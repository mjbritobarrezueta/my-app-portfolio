<FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:gravity="center_horizontal"
        tools:context=".MainActivity">

    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:gravity="center_horizontal"
        tools:context=".MainActivity">

        <TextView
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="32dp"
            android:layout_gravity="center"
            android:text="My Nanodegree Apps!"
            android:textAppearance="?android:textAppearanceLarge"
            android:textColor="#3b9d99"
            android:textStyle="bold" />
\
        <Button
            android:id="@+id/first_app"
            android:layout_below="@id/title"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:text="spotify streamer"
            android:textAllCaps="true"
            android:textStyle="bold"
            android:background="#fbae34"
            android:onClick="displayMessage1"/>

        <Button
            android:id="@+id/second_app"
            android:layout_below="@id/first_app"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:text="scores app"
            android:textAllCaps="true"
            android:textStyle="bold"
            android:background="#fbae34"
            android:onClick="displayMessage2"/>

        <Button
            android:id="@+id/third_app"
            android:layout_below="@id/second_app"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:text="library app"
            android:textAllCaps="true"
            android:textStyle="bold"
            android:background="#fbae34"
            android:onClick="displayMessage3"/>

        <Button
            android:id="@+id/fourth_app"
            android:layout_below="@id/third_app"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:text="build it bigger"
            android:textAllCaps="true"
            android:textStyle="bold"
            android:background="#fbae34"
            android:onClick="displayMessage4"/>

        <Button
            android:id="@+id/fifth_app"
            android:layout_below="@id/fourth_app"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:text="xyz reader"
            android:textAllCaps="true"
            android:textStyle="bold"
            android:background="#fbae34"
            android:onClick="displayMessage5"/>

        <Button
            android:id="@+id/sixth_app"
            android:layout_below="@id/fifth_app"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:text="capstone: my own app"
            android:textAllCaps="true"
            android:textStyle="bold"
            android:textColor="@android:color/white"
            android:background="#ac0b2c"
            android:onClick="displayMessage6"/>

    </RelativeLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="120dp"
        android:padding="8dp"
        android:background="@android:color/darker_gray"
        android:text="This button will lunch my spotify streamer app"
        android:textAppearance="?android:textAppearanceSmall"
        android:visibility="invisible"
        android:id="@+id/first_message"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="173dp"
        android:padding="8dp"
        android:background="@android:color/darker_gray"
        android:text="This button will lunch my scores app"
        android:textAppearance="?android:textAppearanceSmall"
        android:visibility="invisible"
        android:id="@+id/second_message"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="230dp"
        android:padding="8dp"
        android:background="@android:color/darker_gray"
        android:text="This button will lunch my library app"
        android:textAppearance="?android:textAppearanceSmall"
        android:visibility="invisible"
        android:id="@+id/third_message"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="287dp"
        android:padding="4dp"
        android:background="@android:color/darker_gray"
        android:text="This button will lunch my build it bigger app"
        android:textAppearance="?android:textAppearanceSmall"
        android:visibility="invisible"
        android:id="@+id/fourth_message"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="344dp"
        android:padding="4dp"
        android:background="@android:color/darker_gray"
        android:text="This button will lunch my xyz reader app"
        android:textAppearance="?android:textAppearanceSmall"
        android:visibility="invisible"
        android:id="@+id/fifth_message"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="401dp"
        android:padding="4dp"
        android:background="@android:color/darker_gray"
        android:text="This button will lunch my spotify streamer app"
        android:textAppearance="?android:textAppearanceSmall"
        android:visibility="invisible"
        android:id="@+id/sixth_message"
        />

</FrameLayout>
